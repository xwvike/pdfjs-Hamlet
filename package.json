{
  "name": "pdfjs-hamlet",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generic": "cd pdf.js-4.3.136 && gulp generic && cd ..",
    "copy-pdf": "copyfiles -u 1 './pdf.js-4.3.136/build/generic/*' './public/pdf.js/'",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier --write ."
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-svelte"
    ],
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "endOfLine": "lf"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "del": "^7.1.0",
    "postcss": "^8.4.38",
    "prettier": "3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "rollup-plugin-copy": "^3.5.0",
    "rxjs": "^7.8.1",
    "svelte": "^4.2.12",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.0"
  }
}
